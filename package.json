{
  "name": "mckennapsean.github.io",
  "version": "0.9.0",
  "scripts": {
    "install": "run-s sync bower",
    "build": "jekyll build",
    "build:prod": "cross-env JEKYLL_ENV=production npm run build",
    "watch": "jekyll build --watch",
    "serve": "jekyll serve",
    "host": "hs _site/",
    "reload": "browser-sync start --server _site/ -f _site/*.*",
    "start": "run-p watch reload",
    "test": "run-s clean build host",
    "deploy": "run-s clean build:prod host",
    "sync": "git submodule update --init --recursive",
    "bower": "bower install jquery#2.1.1 tablesorter source-sans-pro font-awesome html5shiv --config.directory=vendor",
    "clean": "rm -rf _site/"
  },
  "dependencies": {
    "bower": "^1.7.9",
    "browser-sync": "^2.14.0",
    "cross-env": "^2.0.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.3.0"
  }
}
